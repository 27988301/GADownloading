<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gastudio.gadownloading">

    <application
        android:allowBackup="true"
        android:icon="@drawable/gastudio_icon"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.gastudio.gadownloading.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.gastudio.gadownloading.SuccessActivity"/>
        <activity android:name="com.gastudio.gadownloading.FailedActivity"/>
    </application>

</manifest>